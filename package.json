{
  "name": "ts",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "prisma generate && tsc && tsc-alias",
    "start": "node -r tsconfig-paths/register build/server.js",
    "db:migrate": "npx prisma migrate dev --name user-entity --create-only && npx prisma generate",
    "db:push": "npx prisma db push  && npx prisma generate",
    "db:seed": "ts-node --transpile-only prisma/seed.ts",
    "db:reset": "npx prisma migrate reset",
    "db:generate": "npx prisma generate",
    "postinstall": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "cloudinary-build-url": "^0.2.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@testing-library/jest-dom": "^6.6.3",
